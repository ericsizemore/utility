{
    "name": "esi/utility",
    "description": "Utility - Collection of various PHP utility functions.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Sizemore",
            "email": "admin@secondversion.com",
            "homepage": "https://www.secondversion.com/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "ext-mbstring": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": { "Esi\\Utility\\": "src/Utility" }
    },
    "autoload-dev": {
        "psr-4": {
            "Esi\\Utility\\Tests\\": "tests/src/"
        }
    },
    "scripts": {
        "test": "phpunit --display-warnings --globals-backup"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
